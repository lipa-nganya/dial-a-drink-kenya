{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "local-dev": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "local-dev",
      "env": {
        "EXPO_PUBLIC_ENV": "local",
        "EXPO_PUBLIC_BUILD_PROFILE": "local-dev"
      }
    },
    "cloud-dev": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "cloud-dev",
      "env": {
        "EXPO_PUBLIC_ENV": "cloud",
        "EXPO_PUBLIC_BUILD_PROFILE": "cloud-dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}





